{
  "name": "@repo/db",
  "version": "0.0.0",
  "private": true,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist .turbo",
    "db:generate": "pnpm prisma generate",
    "db:push": "pnpm prisma db push",
    "db:seed": "ts-node prisma/seed.ts",
    "studio": "prisma studio --browser none"
  },
  "dependencies": {
    "@faker-js/faker": "^8.4.1",
    "@prisma/client": "^5.18.0",
    "prisma": "^5.18.0",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@types/node": "^20.11.24",
    "bcryptjs": "^2.4.3",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3"
  }
}
